// Different Datatypes
#include <iostream>

int main() {
    int integerVar = 100;
    float floatVar = 10.5;
    char charVar = 'A';
    bool boolVar = true;
    std::cout << "Integer: " << integerVar << "\nFloat: " << floatVar << "\nCharacter: " << charVar << "\nBoolean: " << boolVar << std::endl;
    return 0;
}
